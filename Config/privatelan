# ----------------------------------------------------------------------------
# @file    privatelan
# @brief   private LAN address definitions for all NGPS software
# @author  David Hale <dhale@caltech.edu>
# @details These will be used by CMake to create the individual .cfg files
#          Listing is in numerical order by IP# to help avoid any conflicts
# ----------------------------------------------------------------------------

set( IP_GATEWAY         10.0.0.1   )

# terminal server
#
set( IP_ETS8P           10.0.0.8   )

# network power switches
#
set( IP_NPS1            10.0.0.11  )
set( IP_NPS2            10.0.0.12  )
set( IP_NPS3            10.0.0.13  )
set( IP_NPS4            10.0.0.14  )

set( IP_LAMP_MODULATOR  10.0.0.36  )

#  flexure motor controllers
#
set( IP_FLEXURE_U       10.0.0.40  )
set( IP_FLEXURE_G       10.0.0.41  )
set( IP_FLEXURE_R       10.0.0.42  )
set( IP_FLEXURE_I       10.0.0.43  )

set( IP_CR1000X         10.0.0.100 )

# Lakeshores
#
set( IP_LKS_UR          10.0.0.136 )
set( IP_LKS_IG          10.0.0.137 )
